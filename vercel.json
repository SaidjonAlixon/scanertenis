{
  "functions": {
    "api/check-ticket.py": {
      "runtime": "python3.9"
    }
  },
  "routes": [
    {
      "src": "/api/check-ticket",
      "dest": "/api/check-ticket.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "DATABASE_URL": "@database_url"
  }
}
